{
    "nrD":1200,
    "rSeed":12345,
    "modules":[
        {
            "name":"DataProviderX",
            "collection":"Distribution",
            "kind":"uniform",
            "domain":[-1,1]
        },
        {
            "name":"DataProviderY",
            "kind":"Group",
            "input":
            {
                "argIn":"DataProviderX"
            },
            "modules":[
                {
                    "name":"CosFreq",
                    "kind":"SignalBuilder",
                    "segments":[
                        {"duration":400, "kind":"constant", "value":3.14},
                        {"duration":200, "kind":"linear",   "start":3.14,"final":6.28},
                        {"duration":600, "kind":"constant", "value":6.28}
                    ]
                },
                {
                    "name":"TargetFunction",
                    "collection":"TargetFunction",
                    "kind":"cosine",
                    "input":
                    {
                        "argIn":"argIn"
                    }
                }
            ]
        },
        {
            "name":"Noise",
            "collection":"AddNoise",
            "kind":"gaussian",
            "std":0.1,
            "input":
            {
                "argIn":"DataProviderY"
            }
        },
        {
            "name":"LearnSys",
            "kind":"IncrementalLearningSystem",
            "input":
            {
                "xEval":"DataProviderX",
                "xLearn":"DataProviderX",
                "yLearn":"Noise"
            }
        },
        {
            "name":"PerfEval",
            "kind":"CumulativeLoss",
            "input":
            {
                "prediction":"LearnSys",
                "target":"Noise"
            }
        },
        {
            "name":"DataSink_Performace",
            "kind":"plot",
            "legend":["Cumulative Loss"],
            "input":
            {
                "y":"PerfEval"
            }
        },
        {
            "name":LearningHistory,
            "collection":"Plotting",
            "kind":"plotLearningHistory",
            "input":
            {
                "xLearn":"DataProviderX",
                "yLearn":"Noise",
                "ILS":"LearnSys",
                "GroundTruth":"DataProviderY"
            }
        }
    ]
}