{
    "nrD":1200,
    "rSeed":12345,
    "modules":[
        {
            "name":"DataProviderX",
            "collection":"Distribution",
            "kind":"uniform",
            "domain":[-1,1]
        },
        {
            "name":"DataProviderY",
            "kind":"Group",
            "input":
            {
                "argIn":"DataProviderX"
            },
            "file":"Nonstationary_Cosine.json"
        },
        {
            "name":"Noise",
            "collection":"AddNoise",
            "kind":"gaussian",
            "std":0.1,
            "input":
            {
                "argIn":"DataProviderY"
            }
        },
        {
            "name":"LearnSys",
            "kind":"IncrementalLearningSystem",
            "input":
            {
                "xEval":"DataProviderX",
                "xLearn":"DataProviderX",
                "yLearn":"Noise"
            }
        },
        {
            "name":"PerfEval",
            "kind":"CumulativeLoss",
            "input":
            {
                "prediction":"LearnSys",
                "target":"Noise"
            }
        },
        {
            "name":"DataSink_Performace",
            "kind":"plot",
            "legend":["Cumulative Loss"],
            "input":
            {
                "y":"PerfEval"
            }
        }
    ]
}