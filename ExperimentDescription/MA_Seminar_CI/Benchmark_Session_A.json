{
    "ID":"BechmarkSuiteA",
    "module":"ModularExperiment",
    "simulationSteps":"TargetIterator",
    "rSeed":12345,
    "parameter":{
                "domain":[0,2]
            },
    "modules":[
        {
            "ID":"TargetIterator",
            "collection":"Iterator",
            "module":"ModuleFilesInFolder",
            "folder":"MA_Seminar_CI"
        },
        {
            "ID":"BenchmarkExperiment",
            "module":"ModularExperiment",
            "simulationSteps":"DataProviderY",
            "rSeed":12345,
            "input":{
                "moduleFile":"TargetIterator"
            },
            "modules":[
                {
                    "ID":"DataProviderX",
                    "collection":"Distribution",
                    "module":"Uniform",
                    "domain":"@domain",
                    "size":[1]
                },
                {
                    "ID":"DataProviderY",
                    "module":"Group",
                    "input":
                    {
                        "argIn":"DataProviderX",
                        "modules":"moduleFile"
                    },
                    "modules":"ModuleDescription/MA_Seminar_CI/Cos_Shift.json"
                },
                {
                    "ID":"Noise",
                    "collection":"AddNoise",
                    "module":"Uniform",
                    "range":0.1,
                    "input":
                    {
                        "value":"DataProviderY"
                    }
                },
                {
                    "ID":"LearnSys",
                    "path":"IncrementalLearning",
                    "collection":"IncrementalLearningSystem",
                    "module":"AdaptiveLinearSimplicialBSpline",
                    "approximator":
                    {
                        "kind":"AdaptiveLinearSimplicialBSpline",
                        "inputs":[{"kind":"UIGLTlinear","res":0.1}]
                    },
                    "learner":{"name":"vSGD"},
                    "input":
                    {
                        "xLearn":"DataProviderX",
                        "yLearn":"Noise"
                    }
                },
                {
                    "ID":"CL_data",
                    "collection":"PerformanceEvaluation",
                    "module":"CumulativeLoss",
                    "input":
                    {
                        "target":"Noise",
                        "prediction":"LearnSys"
                    }
                },
                {
                    "ID":"CL_ground_truth",
                    "collection":"PerformanceEvaluation",
                    "module":"CumulativeLoss",
                    "input":
                    {
                        "target":"DataProviderY",
                        "prediction":"LearnSys"
                    }
                },
                {
                    "ID":"NameSelect",
                    "collection":"Basic",
                    "module":"strcutFilename",
                    "input":
                    {
                        "fullfile":"moduleFile"
                    }
                },
                {
                    "ID":"CL_perf",
                    "collection":"Plotting",
                    "module":"PlotVsTime",
                    "savePath":"Plots/",
                    "format":"pdf",
                    "input":
                    {
                        "CL_data":"CL_data",
                        "CL_gt":"CL_ground_truth",
                        "save":"NameSelect"
                    }
                }
            ]
        }
    ]
}
