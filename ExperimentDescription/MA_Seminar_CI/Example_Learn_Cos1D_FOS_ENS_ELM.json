{
    "ID":"LearnNonstationaryCos",
    "module":"ModularExperiment",
    "nrD":1200,
    "rSeed":12345,
    "parameter":
        {
            "domain":[1,3],
            "nrLayer":4,
            "offset":1,
            "res":2
        },
    "modules":[
        {
            "ID":"DataProviderX",
            "collection":"Distribution",
            "module":"Uniform",
            "domain":"@domain",
            "size":[1]
        },
        {
            "ID":"DataProviderY",
            "module":"Group",
            "input":
            {
                "argIn":"DataProviderX"
            },
            "modules":[
                {
                    "ID":"CosFreq",
                    "path":"Benchmark",
                    "module":"SignalBuilder",
                    "segments":[
                        {"duration":400, "kind":"constant", "value":3.14},
                        {"duration":400, "kind":"linear",   "start":3.14,"final":6.28},
                        {"duration":400, "kind":"constant", "value":6.28}
                    ]
                },
                {
                    "ID":"TargetFunction",
                    "path":"Benchmark",
                    "collection":"TargetFunction",
                    "module":"Cosine",
                    "input":
                    {
                        "x":"argIn",
                        "freq":"CosFreq"
                    }
                }
            ]
        },
        {
            "ID":"Noise",
            "collection":"AddNoise",
            "module":"Uniform",
            "range":0.0,
            "input":
            {
                "value":"DataProviderY"
            }
        },
        {
            "ID":"LearnSys",
            "path":"FOS_ELM",
            "collection":"FOS_ENS_ELM",
            "module":"FOS_ENS_ELM",
            "input":
            {
                "xLearn":"DataProviderX",
                "yLearn":"Noise"
            }
        },
        {
            "ID":"CL_data",
            "collection":"PerformanceEvaluation",
            "module":"CumulativeLoss",
            "input":
            {
                "target":"Noise",
                "prediction":"LearnSys"
            }
        },
        {
            "ID":"CL_ground_truth",
            "collection":"PerformanceEvaluation",
            "module":"CumulativeLoss",
            "input":
            {
                "target":"DataProviderY",
                "prediction":"LearnSys"
            }
        },
        {
            "ID":"LearningHistory",
            "collection":"Plotting",
            "module":"PlotLearningHistory",
            "samplingDomain":"@domain",
            "samplingSize":101,
            "legend":"Training Data",
            "input":
            {
                "xLearn":"DataProviderX",
                "yLearn":"Noise",
                "ILS":"LearnSys",
                "GroundTruth":"DataProviderY"
            }
        },
        {
            "ID":"CL_perf",
            "collection":"Plotting",
            "module":"PlotVsTime",
            "input":
            {
                "CL_data":"CL_data",
                "CL_gt":"CL_ground_truth"
            }
        }
    ]
}